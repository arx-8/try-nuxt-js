{
  "name": "try-nuxt-js",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^12 || ^16",
    "npm": "0",
    "yarn": "^1"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt",
    "eslint-print-config": "eslint --print-config src/index.tsx > misc/eslint-print-config.json",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --max-warnings=0 --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:tsc": "tsc --noEmit",
    "lint": "yarn lint:js && yarn lint:tsc && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:tsc && yarn lint:js --fix && yarn lint:style --fix",
    "start": "nuxt generate && nuxt start",
    "test-ci": "TZ=Asia/Tokyo jest --ci",
    "test": "TZ=Asia/Tokyo jest --watch",
    "update-all": "npx --yes npm-check-updates@12.3.0 -u --reject \"/^(nuxt|vue|webpack|@nuxt/types)$/\""
  },
  "dependencies": {
    "normalize.css": "8.0.1",
    "nuxt": "2.15.8",
    "vue": "2.6.14",
    "vue-server-renderer": "2.6.14"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "8.0.0",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/stylelint-module": "4.1.0",
    "@types/jest": "27.4.0",
    "@vue/runtime-dom": "3.2.31",
    "@vue/test-utils": "1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.5.1",
    "core-js": "3.21.1",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-nuxt": "3.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-vue": "8.4.1",
    "jest": "27.5.1",
    "postcss-html": "1.3.0",
    "prettier": "2.5.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "stylelint": "14.5.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended-vue": "1.3.0",
    "stylelint-config-standard": "25.0.0",
    "ts-jest": "27.1.3",
    "vue-jest": "3.0.7",
    "vue-template-compiler": "2.6.14",
    "vue-tsc": "0.31.3",
    "webpack": "4.46.0"
  }
}
